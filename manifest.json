{
  "name": "Sleed",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Sleed"
  },
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon48.png"
  }
}
